{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
     "logo": {
         "type": "kitty-direct",
         "source": "~/.config/fastfetch/image2.png",
	 "width": 25,
         "height": 12
     },
    "display": {
        "separator": ""
    },
    "modules": [
        "break",
        {
            "type": "title",
            "key": "╭─ ",
            "format": "{1}@{2}",
            "keyColor": "magenta"
