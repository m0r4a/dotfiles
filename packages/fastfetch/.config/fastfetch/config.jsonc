        },

        "break",

        {
            "type": "shell",
            "key": "╭─ ",
            "format": "{1}",
            "keyColor": "blue"
        },
        {
            "type": "terminal",
            "key": "├─ ",
            "format": "{1}",
            "keyColor": "blue"
        },
        {
            "type": "wm",
            "key": "╰─ ",
            "format": "{1}",
            "keyColor": "blue"
        },


        "break",

        {
            "type": "cpu",
            "key": "╭─ ",
            "format": "Intel i7-13650HX",
            "keyColor": "cyan"
        },
        {
            "type": "cpu",
            "key": "├─ ",
            "format": "GeForce RTX 4060M",
            "keyColor": "cyan"
        },
        {
            "type": "command",
            "key": "╰─",
